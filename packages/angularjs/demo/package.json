{
  "name": "@szg/oauth2-authorization-proxy-client-angularjs-demo",
  "version": "1.0.0-rc1",
  "description": "Demo of the AngularJS implementation of the OAuth2 Authorization Proxy Client library",
  "main": "./src/index.js",
  "private": true,
  "repository": "https://github.com/szgabsz91/oauth2-authorization-proxy-client",
  "author": "szgabsz91",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=12.8.1",
    "yarn": ">=1.17.3"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production webpack --env.prod --config ./webpack.config.js",
    "lint": "eslint ./src/**/*.js",
    "lint:fix": "eslint --fix ./src/**/*.js",
    "test": "cross-env NODE_ENV=test karma start ./karma.config.js --single-run",
    "test:watch": "cross-env NODE_ENV=testwatch karma start ./karma.config.js",
    "prestart": "yarn clean",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js"
  },
  "dependencies": {
    "@szg/oauth2-authorization-proxy-client-angularjs-core": "1.0.0-rc1",
    "@szg/oauth2-authorization-proxy-client-angularjs-facebook": "1.0.0-rc1",
    "@szg/oauth2-authorization-proxy-client-angularjs-google": "1.0.0-rc1",
    "@uirouter/angularjs": "1.0.22",
    "angular": "1.7.8",
    "angular-animate": "1.7.8",
    "angular-aria": "1.7.8",
    "angular-material": "1.1.20",
    "angular-mocks": "1.7.8",
    "angular-sanitize": "1.7.8",
    "angular-translate": "2.18.1"
  }
}
